@page 
@model IndexModel
@using PfannenkuchenBot.Commands.Logging
<form method="post">
    <input type="text" asp-for="Username" placeholder="Enter username!">
    <input type="text" asp-for="Command" placeholder="Enter command!">
    <button type=submit>Click here to submit</button>
</form>
<p>
    @Model.responseWrapper
</p>

<ul>
@if (Model.LatestResponses is not null && Model.LatestResponses.Count() < 0)
{
    foreach (CommandLogEntry logEntry in Model.LatestResponses)
    {
        <li>
            @logEntry.Content
        </li>
    }
}
</ul>